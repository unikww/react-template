{
  "private": false,
  "name": "react-template",
  "version": "0.0.1",
  "description": "a React Starter Template by Webpack",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config ./webpack/webpack.dev.mjs",
    "build": "rm -rf dist && NODE_ENV=production webpack --config ./webpack/webpack.prod.mjs",
    "build:github": "rm -rf dist && NODE_ENV=production TARGET=GitHub webpack --config ./webpack/webpack.prod.mjs",
    "serve": "npx serve dist",
    "prepare": "husky install",
    "test": "jest",
    "prettier": "prettier --write src",
    "cspell": "cspell lint 'src/**/*.{js,jsx,ts,tsx}' --no-progress"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "qinghuanI",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "cspell": "^6.27.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "esbuild": "^0.17.10",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.8.4",
    "style-loader": "^3.3.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.29.0",
    "mobx": "^6.8.0",
    "mobx-react": "^7.6.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "setimmediate": "^1.0.5",
    "winston": "^3.8.2"
  }
}
